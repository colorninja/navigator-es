{
  "name": "negotiator-es",
  "description": "HTTP content negotiation",
  "version": "1.0.0",
  "contributors": [
    "Douglas Christopher Wilson <doug@somethingdoug.com>",
    "Federico Romero <federico.romero@outboxlabs.com>",
    "Isaac Z. Schlueter <i@izs.me> (http://blog.izs.me/)",
    "Andrey Tsarev <andrey.tsarev@fpsoftware.io>"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/lib.d.ts",
      "require": "./dist/cjs/lib.js",
      "import": "./dist/esm/lib.js",
      "default": "./dist/esm/lib.js"
    }
  },
  "license": "MIT",
  "keywords": [
    "http",
    "content negotiation",
    "accept",
    "accept-language",
    "accept-encoding",
    "accept-charset"
  ],
  "repository": "jshttp/negotiator",
  "devDependencies": {
    "@types/node": "^18.19.55",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist/",
    "HISTORY.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -b ./tsconfig.json ./tsconfig.cjs.json ./tsconfig.types.json && cp package.esm.json ./dist/esm/",
    "lint": "eslint .",
    "test": "node --test",
    "test-cov": "node --test --experimental-test-coverage",
    "test-lcov": "node --test --experimental-test-coverage --test-reporter=lcov"
  },
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
